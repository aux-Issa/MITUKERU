{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
   "timezone": "Asia/Tokyo",
   "schedule": [
    "every 1 hour after 00:00 and before 23:59 every day"
  ],
    "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all"
    }
  ]
}
